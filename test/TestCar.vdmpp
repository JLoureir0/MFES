    assertEqual(2.5, c.zeroToOneHundred);
    assertEqual(275.5, c.topSpeed);
    assertEqual(380, c.horsePower);
  );

  public testAll() == (
    IO`println("\t car tests");
    testConstructor();
  );
end TestCar