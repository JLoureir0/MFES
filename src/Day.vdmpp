class Day

	types
		public DaySections = seq of Section;
		public DayParticipants = set of Team;

	instance variables
		public sections: DaySections;
		public teams: DayParticipants;

	operations
		public Day(s: seq of Section, t: set of Team) res: Day == (
			sections := s;
			teams := t;
		)
		pre len s > 0 and card t > 1
		post res.sections = s and res.teams = t;
	
end Day